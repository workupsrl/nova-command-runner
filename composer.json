{
    "name": "workup/nova-command-runner",
    "description": "Laravel Nova tool for running Artisan and bash(shell) commands.",
    "keywords": [
        "laravel",
        "nova",
        "Artisan",
        "commands",
        "shell",
        "bash"
    ],
    "license": "MIT",
    "require": {
        "php": ">=8.0",
        "laravel/nova": "^4.0"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Workup\\Nova\\CommandRunner\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Workup\\Nova\\CommandRunner\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
